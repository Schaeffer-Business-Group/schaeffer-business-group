    "stylelint-prettier": "^1.2.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "maxHeaderWidth": 72,
      "maxLineWidth": 80
    }
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{scss}": "stylelint --fix",
    "*.{ts,html,scss,json,md}": "prettier --write"
