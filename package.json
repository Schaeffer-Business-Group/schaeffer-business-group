{
  "name": "schaeffer-business-group",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test:watch": "ng test --watch=true --browsers=ChromeHeadless",
    "test:watch:chrome": "ng test --watch=true --browsers=Chrome"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.9",
